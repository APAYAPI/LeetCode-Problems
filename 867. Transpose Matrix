class Solution {
    public int[][] transpose(int[][] A) {
        int i = A.length, j = A[0].length;
        int[][] result = new int[j][i];
        
        for (int k=0;k<i;k++) {
            for (int l=0;l<j;l++) {
                result[l][k] = A[k][l];
            }
        }
        
        return result;
    }
}
